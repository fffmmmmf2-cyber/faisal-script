-- خدمات
local Workspace = game:GetService("Workspace")

-- أسماء البارتات اللي نبغى نحذفها
local partsToRemove = {"Side 1", "Side 2"}

-- دالة تحذف كل البارتات مهما كانت عددها
local function removeParts(parent)
    for _, obj in pairs(parent:GetDescendants()) do
        if obj:IsA("BasePart") and table.find(partsToRemove, obj.Name) then
            obj:Destroy()
        end
    end
end

-- تنفيذ الحذف
removeParts(Workspace)
