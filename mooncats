-- LocalScript
-- إعدادات البارت
local PART_POSITION = Vector3.new(8.55, 2.75, 76.767)
local PART_SIZE = Vector3.new(6, 6, 6)
local PART_COLOR = BrickColor.new("Bright red")

-- إنشاء البارت
local marker = Instance.new("Part")
marker.Size = PART_SIZE
marker.Position = PART_POSITION
marker.Anchored = true
marker.CanCollide = false
marker.Material = Enum.Material.Neon
marker.BrickColor = PART_COLOR
marker.Parent = workspace
