local Players = game:GetService("Players")
local plr = Players.LocalPlayer

-- مسار ريموت HD Admin
local remote = game.ReplicatedStorage:WaitForChild("HDAdminHDClient").Signals.ExecuteClientCommand

-- نجرب Jump
remote:FireServer("jump", plr.Name)
task.wait(1)

-- نجرب Freeze
remote:FireServer("freeze", plr.Name)
task.wait(1)

-- نجرب Kill
remote:FireServer("kill", plr.Name)
