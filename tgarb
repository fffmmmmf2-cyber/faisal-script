local player = game.Players.LocalPlayer

-- زر الـ GUI
local coalButton = script.Parent -- افترض إن السكربت داخل الزر نفسه

coalButton.MouseButton1Click:Connect(function()
    local character = player.Character
    if not character or not character.PrimaryPart then return end
    
    for _, obj in pairs(workspace:GetDescendants()) do
        if obj.Name == "Coal" and obj:IsA("BasePart") then
            obj.CFrame = character.PrimaryPart.CFrame + Vector3.new(0,5,0)
        end
    end
end)
